{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [
    "**/.env.*local",
    "**/.env"
  ],
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": []
    },
    "lint": {},
    "dev": {
      "cache": false,
      "persistent": true,
      "env": [
        "DATABASE_LOCATION",
        "MODPACK_LOCATION",
        "MINECRAFT_VERSION"
      ]
    }
  }
}